{
  "name": "react-template",
  "version": "0.0.1",
  "description": "A react template project",
  "main": "gulpfile.babel.js",
  "scripts": {
    "test": "BABEL_JEST_STAGE=0 ./node_modules/.bin/jest",
    "autotest": "gulp autotest",
    "start": "./node_modules/.bin/gulp",
    "start-ie": "NODE_ENV=ie ./node_modules/.bin/gulp",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack -p",
    "repl": "./node_modules/.bin/babel-node"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils"
    ]
  },
  "dependencies": {
    "console-polyfill": "^0.2.2",
    "es5-shim": "^4.3.1",
    "es6-shim": "^0.33.11",
    "eventsource-polyfill": "^0.9.6",
    "history": "^1.13.0",
    "html5shiv": "^3.7.3",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-router": "^1.0.0",
    "velocity-animate": "^1.2.3",
    "velocity-react": "^1.1.1"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "css-loader": "^0.22.0",
    "express": "^4.13.3",
    "file-loader": "^0.8.4",
    "gulp": "^3.9.0",
    "gulp-util": "^3.0.7",
    "html-webpack-plugin": "^1.6.2",
    "jest-cli": "^0.5.10",
    "object-assign": "^4.0.1",
    "react-addons-test-utils": "^0.14.2",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "source-map-support": "^0.3.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.4",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.12.1",
    "webpack-hot-middleware": "^2.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pmh/react-template.git"
  },
  "keywords": [
    "react",
    "skeleton",
    "webpack"
  ],
  "author": "Patrik Hedman",
  "bugs": {
    "url": "https://github.com/pmh/react-template/issues"
  }
}
